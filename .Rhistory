library(MASS)
data(crabs)
# dim(crabs)          # [1] 200   8
# head(crabs)
#   sp  sex  index   FL   RW    CL    CW    BD
# 1  B   M     1    8.1   6.7   16.1  19.0  7.0
# 2  B   M     2    8.8   7.7   18.1  20.8  7.4
# 3  B   M     3    9.2   7.8   19.0  22.4  7.7
# 4  B   M     4    9.6   7.9   20.1  23.1  8.2
# 5  B   M     5    9.8   8.0   20.3  23.0  8.2
